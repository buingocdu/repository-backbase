<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.backbase</groupId>
  <artifactId>comments-bom</artifactId>
  <version>2024.03</version>
  <packaging>pom</packaging>
  <name>Comments - BOM</name>
  <description>Bill of materials for the Comments capability</description>
  <organization>
    <name>Backbase</name>
    <url>https://www.backbase.com</url>
  </organization>
  <properties>
    <comments.version>3.0.16</comments.version>
  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>com.backbase.comments</groupId>
        <artifactId>comments</artifactId>
        <version>${comments.version}</version>
      </dependency>
      <dependency>
        <groupId>com.backbase.comments</groupId>
        <artifactId>comments</artifactId>
        <version>${comments.version}</version>
        <type>zip</type>
        <classifier>liquibase</classifier>
      </dependency>
      <dependency>
        <groupId>com.backbase.comments</groupId>
        <artifactId>comments</artifactId>
        <version>${comments.version}</version>
        <type>zip</type>
        <classifier>api</classifier>
      </dependency>
      <dependency>
        <groupId>com.backbase.comments</groupId>
        <artifactId>comments-starter</artifactId>
        <version>${comments.version}</version>
      </dependency>
      <dependency>
        <groupId>com.backbase.comments</groupId>
        <artifactId>comments</artifactId>
        <version>${comments.version}</version>
        <type>json</type>
        <classifier>meta</classifier>
      </dependency>
    </dependencies>
  </dependencyManagement>
  <dependencies>
    <dependency>
      <groupId>com.backbase.comments</groupId>
      <artifactId>comments</artifactId>
      <version>3.0.16</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.backbase.comments</groupId>
      <artifactId>comments</artifactId>
      <version>3.0.16</version>
      <type>zip</type>
      <classifier>api</classifier>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.backbase.comments</groupId>
      <artifactId>comments</artifactId>
      <version>3.0.16</version>
      <type>zip</type>
      <classifier>liquibase</classifier>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.backbase.comments</groupId>
      <artifactId>comments-starter</artifactId>
      <version>3.0.16</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.backbase.comments</groupId>
      <artifactId>comments</artifactId>
      <version>3.0.16</version>
      <type>json</type>
      <classifier>meta</classifier>
      <scope>compile</scope>
    </dependency>
  </dependencies>
</project>
