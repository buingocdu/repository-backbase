<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.backbase</groupId>
  <artifactId>user-lifecycle-bom</artifactId>
  <version>2024.03</version>
  <packaging>pom</packaging>
  <name>User Lifecycle BOM</name>
  <description>Bill of materials for the User Lifecycle capability</description>
  <organization>
    <name>Backbase</name>
    <url>https://www.backbase.com</url>
  </organization>
  <properties>
    <user-lifecycle.version>2.0.9</user-lifecycle.version>
  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>com.backbase.userlifecycle</groupId>
        <artifactId>user-lifecycle</artifactId>
        <version>${user-lifecycle.version}</version>
      </dependency>
      <dependency>
        <groupId>com.backbase.userlifecycle</groupId>
        <artifactId>user-lifecycle</artifactId>
        <version>${user-lifecycle.version}</version>
        <classifier>classes</classifier>
      </dependency>
      <dependency>
        <groupId>com.backbase.userlifecycle</groupId>
        <artifactId>user-lifecycle</artifactId>
        <version>${user-lifecycle.version}</version>
        <type>zip</type>
        <classifier>api</classifier>
      </dependency>
      <dependency>
        <groupId>com.backbase.userlifecycle</groupId>
        <artifactId>user-lifecycle</artifactId>
        <version>${user-lifecycle.version}</version>
        <type>zip</type>
        <classifier>events</classifier>
      </dependency>
      <dependency>
        <groupId>com.backbase.userlifecycle</groupId>
        <artifactId>user-lifecycle</artifactId>
        <version>${user-lifecycle.version}</version>
        <type>json</type>
        <classifier>meta</classifier>
      </dependency>
    </dependencies>
  </dependencyManagement>
  <dependencies>
    <dependency>
      <groupId>com.backbase.userlifecycle</groupId>
      <artifactId>user-lifecycle</artifactId>
      <version>2.0.9</version>
    </dependency>
    <dependency>
      <groupId>com.backbase.userlifecycle</groupId>
      <artifactId>user-lifecycle</artifactId>
      <version>2.0.9</version>
      <classifier>classes</classifier>
    </dependency>
    <dependency>
      <groupId>com.backbase.userlifecycle</groupId>
      <artifactId>user-lifecycle</artifactId>
      <version>2.0.9</version>
      <type>zip</type>
      <classifier>api</classifier>
    </dependency>
    <dependency>
      <groupId>com.backbase.userlifecycle</groupId>
      <artifactId>user-lifecycle</artifactId>
      <version>2.0.9</version>
      <type>zip</type>
      <classifier>events</classifier>
    </dependency>
    <dependency>
      <groupId>com.backbase.userlifecycle</groupId>
      <artifactId>user-lifecycle</artifactId>
      <version>2.0.9</version>
      <type>json</type>
      <classifier>meta</classifier>
    </dependency>
  </dependencies>
</project>
